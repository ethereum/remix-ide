msgstr "Assert.lesserThan(value1, value2[, message])"

#: ../../assert_library.md:11
msgid "Assert"
msgstr "Assert"

#: ../../assert_library.md:14
msgid "value: <bool>"
msgstr "valeur : <bool>"

